REPOS_DIR=  # local absolute path to the directory containing cloned repositories
DEVICE_ID=  # GPU device ID to use (e.g., 0 or 1)
MAX_RETRY=  # max number of retries on API request failure
MAX_WAIT_RESPONSE=  # max wait time in seconds for API response
OUTPUT_DIR=  # directory to store simulation output results
LOG_DIR=  # directory to store log files
OPENAI_TOKEN=  # OpenAI-compatible API token
OPENAI_BASE_URL=  # base URL for the OpenAI-compatible API endpoint
ANTHROPIC_API_KEY=  # Anthropic API key
ANTHROPIC_BASE_URL=  # base URL for the Anthropic API endpoint
GITHUB_TOKENS=  # comma-separated list of GitHub personal access tokens
